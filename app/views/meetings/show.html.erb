<div class="container">
  <div class="row">

      <div class="col-12 col-lg-6">
        <% if @meeting.finish %>
          <%= render 'shared/wrapupright' %>
        <% elsif @meeting.start %>
          <%= render 'shared/transcription' %>
        <% else %>
          <%= render 'shared/agenda' %>
        <% end %>
      </div>
      <div class="col-12 col-lg-6">
        <div class="d-flex justify-content-end">
        <% if@meeting.finish %>
        <%= render 'shared/wrapupleft' %>

        <% elsif @meeting.start %>
          <%= render 'shared/inprogress' %>
        <% else %>
          <%= render 'shared/premeeting' %>
        <% end %>
      </div>
      </div>
  </div>
</div>
